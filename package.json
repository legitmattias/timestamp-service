{
  "name": "timestamp-service",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev src/index.ts",
    "build": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format:code": "prettier --write '**/*.{js,ts,sol}'",
    "compile:contracts": "npx hardhat compile",
    "clean:contracts": "npx hardhat clean",
    "test:contracts": "npx hardhat test",
    "test:contracts:verbose": "npx hardhat test --verbose",
    "test": "echo \"Error: no test specified. To run tests on contracts, run test:contracts\" && exit 1",
    "start:hardhat": "npx hardhat node",
    "deploy:contracts": "npx hardhat run scripts/deploy.ts",
    "deploy:contracts:localhost": "npx hardhat run scripts/deploy.ts --network localhost",
    "deploy:contracts:testnet": "npx hardhat run scripts/deploy.ts --network goerli",
    "list:accounts": "npx hardhat accounts",
    "hardhat:help": "npx hardhat help",
    "generate:types": "npx hardhat typechain",
    "test:coverage": "npx hardhat coverage",
    "report:gas": "npx hardhat test --gas-report"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "dotenv": "^16.4.7",
    "ethers": "^6.13.5",
    "express": "^4.21.2",
    "mongoose": "^8.9.5"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.7",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint": "^9.18.0",
    "globals": "^15.14.0",
    "hardhat": "^2.22.18",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
}
